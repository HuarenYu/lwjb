<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lwjb.tour.daos.OrderDao">
	<sql id="orderFields">
		id,
		name,
		price,
		description,
		details,
		images, 
		booked_dates, 
		shop_id, 
		status, 
		add_time,
		update_time
	</sql>
	<select id="findAll"
			resultType="com.lwjb.tour.models.Item">
		select * from items
	</select>
	<insert id="save"
			parameterType="com.lwjb.tour.models.Item">
		insert into items
			()
		values
			()
	</insert>
</mapper>